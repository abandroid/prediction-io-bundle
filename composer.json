{
    "name": "endroid/prediction-io-bundle",
    "description": "Endroid Prediction IO Bundle",
    "keywords": ["endroid", "prediction", "io", "symfony", "bundle", "php"],
    "homepage": "https://github.com/endroid/prediction-io-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "endroid/prediction-io": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\PredictionIoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\PredictionIoBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
